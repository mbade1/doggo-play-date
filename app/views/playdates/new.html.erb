<h1>Add a New Playdate!</h1>
<%= form_for @playdate do |f| %>
    <%= f.label :date %>
    <%= f.date_field :date %><br><br>
    <%= f.label :time %>
    <%= f.time_field :time %><br><br>
    <p>Select your park:</p>

    <%= f.submit "Add Playdate!" %>
<% end %>


